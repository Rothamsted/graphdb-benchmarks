g.V().hasLabel ( 'Concept:Protein:Resource' )
  .has ( 'prefName', regex ( "(?i).*$protWord.*" ) ).as ( 'p' )
.out ( 'is_part_of' ).hasLabel ( 'Concept:Protcmplx:Resource' ).as ( 'cpx1' )
.out ( 'is_part_of' ).hasLabel ( 'Concept:Protcmplx:Resource' ).as ( 'cpx2' )
.select ( 'p', 'cpx1', 'cpx2' )
.by ( coalesce ( values ( 'prefName' ), constant ( '' ) ) )
.order ().by ( shuffle )
.limit ( 100 )
